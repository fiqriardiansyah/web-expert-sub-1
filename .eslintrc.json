{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/no-extraneous-dependencies": "off",
        "no-unused-vars": "warn",
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off",
        "max-len": "off"
    }
}
